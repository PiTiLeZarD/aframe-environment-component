{
    "name": "aframe-environment-component",
    "version": "2.0.0-alpha.1",
    "description": "Infinite environments for your A-Frame VR scene in just one file.",
    "main": "dist/aframe-environment-component.js",
    "cdn": "dist/aframe-environment-component.min.js",
    "scripts": {
        "dev": "parcel src/index.html",
        "clean": "rm -Rf .parcel-cache dist",
        "predist": "yarn clean",
        "dist": "parcel build src/index.js",
        "postdist": "minify dist/aframe-environment-component.js > dist/aframe-environment-component.min.js",
        "prepublish": "yarn dist",
        "ghpages": "ghpages"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PiTiLeZarD/aframe-environment-component.git"
    },
    "keywords": [
        "aframe",
        "aframe-component",
        "aframe-vr",
        "vr",
        "mozvr",
        "webvr",
        "environment"
    ],
    "author": "Diego F. Goberna <github@feiss.be>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/PiTiLeZarD/aframe-environment-component/issues"
    },
    "homepage": "https://github.com/PiTiLeZarD/aframe-environment-component#readme",
    "devDependencies": {
        "minify": "^8.0.4"
    },
    "dependencies": {
        "parcel": "^2.5.0"
    }
}
